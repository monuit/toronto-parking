{
  "name": "map-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=22.12.0"
  },
  "scripts": {
    "dev": "node scripts/run-with-heap.mjs node_modules/vite/bin/vite.js",
    "dev:ssr": "node scripts/run-with-heap.mjs server/index.js",
    "build": "node scripts/run-with-heap.mjs node_modules/vite/bin/vite.js build",
    "build:ssr": "node scripts/run-with-heap.mjs node_modules/vite/bin/vite.js build && node scripts/run-with-heap.mjs node_modules/vite/bin/vite.js build --ssr src/entry-server.jsx --outDir dist/server",
    "start": "node scripts/run-with-heap.mjs server/index.js",
    "lint": "eslint .",
    "preview": "node scripts/run-with-heap.mjs node_modules/vite/bin/vite.js preview",
    "serve:ssr": "node scripts/run-with-heap.mjs server/index.js",
    "smoke:overlay": "node scripts/run-with-heap.mjs scripts/run-overlay-smoke-test.mjs",
  "cache:tickets": "node scripts/run-with-heap.mjs scripts/pushTicketsToRedis.mjs",
  "build:ward-tiles": "node scripts/run-with-heap.mjs scripts/build-ward-tiles.mjs",
  "pmtiles:build": "python ../scripts/pmtiles/build_pmtiles.py --output-dir ../pmtiles",
  "pmtiles:upload": "node ../scripts/pmtiles/upload_to_minio.mjs --dir ../pmtiles"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.699.0",
    "@turf/boolean-point-in-polygon": "^7.0.0",
    "@turf/helpers": "^7.0.0",
    "dotenv": "^17.2.3",
    "express": "^4.21.1",
    "flatbush": "^4.0.2",
    "geojson-vt": "^3.2.1",
    "lru-cache": "^11.1.0",
    "maplibre-gl": "^5.8.0",
    "pmtiles": "^4.3.0",
    "pg": "^8.16.3",
    "prop-types": "^15.8.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-map-gl": "^8.1.0",
    "redis": "^5.8.3",
    "supercluster": "^8.0.1",
    "vt-pbf": "^3.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "playwright": "^1.56.0",
    "vite": "^7.1.7"
  }
}
